{
  "name": "sveltekit-threlte-3d-galaxy",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "pnpm check && pnpm lint && pnpm format && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint --fix . && stylelint --fix **/*.pcss",
    "format": "prettier --write .",
    "prepare": "husky install",
    "pre-commit": "pnpm check && lint-staged"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.20.2",
    "@threlte/core": "6.0.0-next.8",
    "@threlte/extras": "5.0.0-next.13",
    "@types/node": "^20.3.0",
    "@types/three": "^0.152.1",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-svelte": "^2.30.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "3.59.1",
    "svelte-check": "^3.4.3",
    "svelte-preprocess": "^5.0.4",
    "three": "^0.153.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "stylelint": "^15.6.2"
      }
    }
  },
  "type": "module"
}
